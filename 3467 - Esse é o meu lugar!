#include <bits/stdc++.h>

using namespace std;

int main()
{
    string palavra;
	ios_base :: sync_with_stdio(false);
	cin.tie(0);
	cout.tie(0);
	
	while (cin >> palavra){

        if (palavra == "xxL")
            cout << "Esse eh o meu lugar\n";
        else 
            cout << "Oi, Leonard\n";
	
	}
    return 0;
}
